{
  "name": "log4js-knex",
  "version": "0.2.1",
  "description": "A Knex.js backend for log4js, with sqlite3/mysql support",
  "keywords": [
    "log4js",
    "knex",
    "postgres",
    "mysql",
    "sqlite3"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:morungos/log4js-knex.git"
  },
  "author": "Stuart Watt <stuart@morungos.com>",
  "homepage": "https://github.com/morungos/log4js-knex",
  "license": "MIT",
  "main": "./lib/log4js-knex",
  "dependencies": {},
  "peerDependencies": {
    "log4js": "3.x",
    "knex": "^0.13"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "log4js": "^3.0.6"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build/"
    ],
    "collectCoverageFrom": [
      "lib/**/*.js"
    ]
  }
}
