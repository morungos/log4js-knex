{
  "name": "log4js-knex",
  "version": "0.1.0",
  "description": "A Knex.js backend for log4js, with sqlite3/mysql support",
  "keywords": [
    "log4js",
    "knex",
    "postgres",
    "mysql",
    "sqlite3"
  ],
  "repository": "git@github.com:log4js-knex.git",
  "author": "Stuart Watt <stuart@morungos.com>",
  "homepage": "https://github.com/morungos/log4js-knex",
  "engines": {
    "node": ">= 0.10.0"
  },
  "main": "./index.js",
  "dependencies": {},
  "peerDependencies": {
    "log4js": "3.x",
    "knex": "^0.13"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "jest": "^23.6.0",
    "knex": "^0.15.2",
    "log4js": "^3.0.6",
    "sqlite3": "^4.0.2"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build/"
    ],
    "collectCoverageFrom": [
      "lib/**/*.js"
    ]
  }
}
